{
  "cli": {
    "version": ">= 16.18.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production"
    },
    "simulator": {
      "extends": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "channel": "simulator"
    },
    "android": {
      "extends": "development",
      "android": {
        "buildType": "apk"
      },
      "channel": "android"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6754826042"
      }
    }
  }
}
